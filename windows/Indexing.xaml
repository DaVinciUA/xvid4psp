<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2006" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="XviD4PSP.Indexing"
	x:Name="Window"
	Title="Indexing"
	Width="298" Height="90" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Closing="Window_Closing">
	<Grid x:Name="LayoutRoot" Background="{DynamicResource black_gradient_2}" Width="292" Height="64">
		<ProgressBar x:Name="prCurrent" Foreground="{DynamicResource StrawBerryBrush}" Margin="8,8,8,8" VerticalAlignment="Bottom" Height="12" IsIndeterminate="True"/>
		<Label OpacityMask="{x:Null}" x:Name="label_info" Content="Indexing..." Background="{x:Null}" BorderBrush="{x:Null}" FontSize="14" Foreground="#FFFFFFFF" Height="Auto" d:LayoutOverrides="Width" HorizontalAlignment="Left" Margin="8,8,0,0" VerticalAlignment="Top"/>
	</Grid>
</Window>